  {
      "name": "Hello Extensionnn",
      "description": "Base Level Extension",
      "version": "1.0",
      "manifest_version": 2,
      "browser_action":
      {
          "default_popup": "popup.html"
      },
      "content_scripts": [
      {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["popup.js"]
      }],
      "permissions": [
          "activeTab",
          "storage",
          "tabs"
      ]
  }